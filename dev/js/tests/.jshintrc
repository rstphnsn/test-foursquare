{
    "bitwise": true,
    "browser": true,
    "devel": true,
    "curly": true,
    "eqeqeq": true,
    "es3": true, /* Assume site must work in IE6/7/8. */
    "forin": true,
    "freeze": true,
    "globals": { /* Require and Jasmine's Globals. Note that $ is not permitted. */
        "define": false,
        "require": false,
        "describe": false, /* Jasmine */
        "xdescribe": false,
        "it": false,
        "xit": false,
        "beforeEach": false,
        "afterEach": false,
        "jasmine": false,
        "spyOn": false,
        "expect": false,
        "waitsFor": false,
        "inject": false, /* angular mock */
        "module": false /* angular mock */
    },
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": false,
    "nonew": true,
    "onevar": true,
    "plusplus": false,
    "regexp": false,
    "regexdash": true,
    "strict": false,
    "trailing": true, /* Turn 'show whitespace' on in your editor. */
    "undef": true,
    "unused": true,
    "globalstrict": true
}
